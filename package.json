{
  "author": "Niko Pettersen",
  "dependencies": {
    "@nuxtjs/axios": "^5.11.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "nuxt": "^2.12.2"
  },
  "description": "Nuxt.js build with TypeScript and TSX support",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.4.1",
    "@nuxt/typescript-build": "^1.0.3",
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-config-typescript": "^2.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@types/jest": "^26.0.0",
    "@vue/babel-preset-app": "^4.4.4",
    "@vue/composition-api": "^0.6.3",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-preset-vca-jsx": "^0.3.5",
    "canvas": "^2.6.1",
    "cypress": "^4.8.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-nuxt": ">=1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-serializer-vue": "^2.0.2",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.2.10",
    "prettier": "^2.0.5",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "start-server-and-test": "^1.11.0",
    "stylelint": "^13.6.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^26.1.0",
    "vue-jest": "^4.0.0-0",
    "vuepress": "^1.5.2"
  },
  "engines": {
    "node": "~14.2.0",
    "npm": "~6.14.0"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,ts,tsx}": "npm run lint:fix"
  },
  "name": "nuxt-tsx",
  "private": true,
  "resolutions": {
    "minimist": "^1.2.5"
  },
  "scripts": {
    "build": "nuxt build",
    "cypress:ci": "CYPRESS_CI=true cypress run --config-file .cypress.json --config video=false",
    "cypress:run": "cypress run --config-file .cypress.json",
    "dev": "nuxt",
    "docs:build": "vuepress build docs",
    "docs:deploy": "./scripts/deploy-docs.sh",
    "docs:dev": "vuepress dev docs",
    "generate": "nuxt generate",
    "lint:css": "stylelint --ignore-path .gitignore '**/*.scss'",
    "lint:fix:css": "stylelint --ignore-path .gitignore --fix '**/*.scss'",
    "lint:fix:source": "eslint --ext .js,.json,.ts,.tsx --fix --ignore-path .gitignore .",
    "lint:fix": "npm run lint:fix:css && npm run lint:fix:source",
    "lint:source": "eslint --ext .js,.json,.ts,.tsx --ignore-path .gitignore .",
    "lint": "npm run lint:css && npm run lint:source",
    "postinstall": "scripts/postinstall.sh",
    "start": "nuxt start",
    "test:ci": "jest && nuxt generate && start-test start 3000 cypress:ci",
    "test:e2e": "nuxt generate && start-test start 3000 cypress:run",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --coverage",
    "test:update": "npm run test:unit -- -u"
  },
  "version": "1.0.2"
}
