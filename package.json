{
  "author": "Niko Pettersen",
  "dependencies": {
    "@nuxtjs/axios": "^5.13.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.3.5",
    "nuxt": "^2.14.12"
  },
  "description": "Nuxt.js build with TypeScript and TSX support",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.5.0",
    "@nuxt/types": "^2.14.12",
    "@nuxt/typescript-build": "^2.0.4",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@types/jest": "^26.0.20",
    "@vue/babel-preset-app": "^4.5.11",
    "@vue/composition-api": "^0.6.7",
    "@vue/test-utils": "^1.1.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-preset-vca-jsx": "^0.3.6",
    "canvas": "^2.6.1",
    "cypress": "^6.4.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-nuxt": ">=2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "sass": "^1.32.6",
    "sass-loader": "^10.1.1",
    "start-server-and-test": "^1.12.0",
    "stylelint": "^13.9.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^26.5.0",
    "vue-jest": "^4.0.0-0",
    "vuepress": "^1.8.0"
  },
  "engines": {
    "node": "~14.8.0",
    "npm": "~7.52.0"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,ts,tsx}": "npm run lint:fix"
  },
  "name": "nuxt-tsx",
  "private": true,
  "resolutions": {
    "minimist": "^1.2.5"
  },
  "scripts": {
    "build": "nuxt build",
    "cypress:ci": "CYPRESS_CI=true cypress run --config-file .cypress.json --config video=false",
    "cypress:run": "cypress run --config-file .cypress.json",
    "dev": "nuxt",
    "docs:build": "vuepress build docs",
    "docs:deploy": "./scripts/deploy-docs.sh",
    "docs:dev": "vuepress dev docs",
    "generate": "nuxt generate",
    "lint": "npm run lint:css && npm run lint:source",
    "lint:css": "stylelint --ignore-path .gitignore '**/*.scss'",
    "lint:fix": "npm run lint:fix:css && npm run lint:fix:source",
    "lint:fix:css": "stylelint --ignore-path .gitignore --fix '**/*.scss'",
    "lint:fix:source": "eslint --ext .js,.json,.ts,.tsx --fix --ignore-path .gitignore .",
    "lint:source": "eslint --ext .js,.json,.ts,.tsx --ignore-path .gitignore .",
    "postinstall": "scripts/postinstall.sh",
    "start": "nuxt start",
    "test": "npm run test:unit && npm run test:e2e",
    "test:ci": "jest && nuxt generate && start-test start 3000 cypress:ci",
    "test:e2e": "nuxt generate && start-test start 3000 cypress:run",
    "test:unit": "jest --coverage",
    "test:update": "npm run test:unit -- -u"
  },
  "version": "1.0.2"
}
