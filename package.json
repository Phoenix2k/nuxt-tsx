{
  "author": "Niko Pettersen",
  "dependencies": {
    "@nuxtjs/axios": "^5.9.7",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "nuxt": "^2.12.2"
  },
  "description": "Nuxt.js build with TypeScript and TSX support",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.3",
    "@nuxt/typescript-build": "^0.6.1",
    "@nuxtjs/eslint-config": "^2.0.2",
    "@nuxtjs/eslint-config-typescript": "^1.0.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@nuxtjs/stylelint-module": "^3.2.2",
    "@types/jest": "^25.1.5",
    "@vue/babel-preset-app": "^4.2.3",
    "@vue/composition-api": "^0.5.0",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.6",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-preset-vca-jsx": "^0.3.4",
    "cypress": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-nuxt": ">=0.5.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "jest-serializer-vue": "^2.0.2",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.1.1",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.2",
    "sass-loader": "^8.0.2",
    "start-server-and-test": "^1.10.11",
    "stylelint": "^13.2.1",
    "stylelint-order": "^4.0.0",
    "ts-jest": "^25.3.0",
    "vue-jest": "^4.0.0-0",
    "vuepress": "^1.4.0"
  },
  "engines": {
    "node": "~12.13.0",
    "npm": "~6.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,ts,tsx}": "npm run lint:fix"
  },
  "name": "nuxt-tsx",
  "private": true,
  "resolutions": {
    "minimist": "^1.2.5"
  },
  "scripts": {
    "build": "nuxt build",
    "cypress:ci": "CYPRESS_CI=true cypress run --config-file .cypress.json --config video=false",
    "cypress:run": "cypress run --config-file .cypress.json",
    "dev": "nuxt",
    "docs:build": "vuepress build docs",
    "docs:deploy": "./scripts/deploy-docs.sh",
    "docs:dev": "vuepress dev docs",
    "generate": "nuxt generate",
    "lint:css": "stylelint --ignore-path .gitignore '**/*.scss'",
    "lint:fix:css": "stylelint --ignore-path .gitignore --fix '**/*.scss'",
    "lint:fix:source": "eslint --ext .js,.json,.ts,.tsx --fix --ignore-path .gitignore .",
    "lint:fix": "npm run lint:fix:css && npm run lint:fix:source",
    "lint:source": "eslint --ext .js,.json,.ts,.tsx --ignore-path .gitignore .",
    "lint": "npm lint:css && npm run lint:source",
    "postinstall": "scripts/postinstall.sh",
    "start": "nuxt start",
    "test:ci": "jest && nuxt generate && start-test start 3000 cypress:ci",
    "test:e2e": "nuxt generate && start-test start 3000 cypress:run",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --coverage",
    "test:update": "npm run test:unit -- -u"
  },
  "version": "1.0.2"
}
