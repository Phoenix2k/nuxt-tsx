{
  "name": "nuxt-tsx",
  "version": "1.0.0",
  "description": "Nuxt.js build with TypeScript and TSX support",
  "author": "Niko Pettersen <niko.pettersen@newthings.co> (https://www.newthings.co)",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "nuxt": "^2.10.2",
    "@nuxtjs/axios": "^5.8.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@nuxtjs/dotenv": "^1.4.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-nuxt": ">=0.5.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "@nuxtjs/stylelint-module": "^3.1.1",
    "stylelint": "^12.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "vue-jest": "^4.0.0-0"
  }
}
